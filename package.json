{
  "name": "kifkaf-app",
  "version": "1.1.4",
  "buildNumber": "16",
  "description": "KifKaf - Feelings Journal - Log moments & meet your needs",
  "productName": "KifKaf",
  "author": "na",
  "private": true,
  "scripts": {
    "start": "quasar dev -m pwa",
    "api": "cd backend && DEBUG=backend:* npm start",
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "dev": "quasar dev -m pwa",
    "build": "quasar build -m pwa",
    "build:capacitor": "quasar build -m capacitor -T ios",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev -m pwa\" http-get://localhost:9200 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev -m pwa\" http-get://localhost:9200 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.9",
    "@sentry/vite-plugin": "^2.10.3",
    "@sentry/vue": "7.98.0",
    "axios": "^1.6.7",
    "axios-retry": "^4.0.0",
    "chart.js": "^4.4.1",
    "firebase": "^10.7.2",
    "pinia": "^2.1.7",
    "quasar": "^2.14.3",
    "swiper": "^11.0.5",
    "vue": "^3.4.15",
    "vue-chartjs": "^5.3.0",
    "vue-i18n": "^9.9.0",
    "vue-router": "^4.2.5",
    "vue3-lottie": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@quasar/app-vite": "^1.7.3",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^5.2.0",
    "autoprefixer": "^10.4.17",
    "babel-loader": "^9.1.3",
    "cypress": "^13.6.3",
    "cypress-terminal-report": "^5.3.12",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.20.1",
    "playwright-webkit": "^1.41.1",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "webpack": "^5.90.0",
    "workbox-build": "^7.0.0",
    "workbox-cacheable-response": "^7.0.0",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
