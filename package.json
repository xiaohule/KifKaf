{
  "name": "kifkaf-app",
  "version": "1.0.1",
  "description": "The social network between you & you",
  "productName": "KifKaf",
  "author": "na",
  "private": true,
  "scripts": {
    "start": "quasar dev -m pwa",
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "dev": "quasar dev -m pwa",
    "build": "quasar build -m pwa",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9200 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9200 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.7",
    "axios": "^1.5.1",
    "firebase": "^10.4.0",
    "firebaseui": "^6.1.0",
    "imap": "^0.8.19",
    "lodash.uniqueid": "^4.0.1",
    "mailparser": "^3.6.5",
    "nodemailer": "^6.9.5",
    "pinia": "^2.1.6",
    "quasar": "^2.12.7",
    "swiper": "^10.3.1",
    "tippy.js": "^6.3.7",
    "vue": "^3.3.4",
    "vue-at": "^3.0.0-alpha.2",
    "vue-router": "^4.2.5",
    "vue-slider-component": "^4.1.0-beta.7",
    "vue3-lottie": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.23.0",
    "@quasar/app-vite": "^1.6.2",
    "@quasar/quasar-app-extension-testing": "^2.1.1",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^5.1.1",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "cypress": "^12.17.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.17.0",
    "playwright-webkit": "^1.38.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "webpack": "^5.88.2",
    "workbox-build": "^6.5.4",
    "workbox-cacheable-response": "^6.5.4",
    "workbox-core": "^6.5.4",
    "workbox-expiration": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
